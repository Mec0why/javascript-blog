{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended","prettier"],   
	"parserOptions": {
        "ecmaVersion": 2015
    },
	"globals": {
  	"Handlebars": false
	},
    "rules": {
        "linebreak-style": [
            "off"
        ],
        "no-console": [
            "off"
        ],
        "linebreak-style": [
            "off"
        ],
        "quotes": [
            "error",
            "single",
            {"allowTemplateLiterals": true}
        ],
        "no-console": [
            "off"
        ]
    }
}
